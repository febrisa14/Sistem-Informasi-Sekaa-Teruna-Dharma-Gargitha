    "routes": [
        {
            "src": "/auth/(.*)",
            "dest": "/public/auth/"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/"
        },
        {
            "src": "/avatar/(.*)",
            "dest": "/public/avatar/"
        },
        {
            "src": "/baju/(.*)",
            "dest": "/public/baju/"
        },
        {
